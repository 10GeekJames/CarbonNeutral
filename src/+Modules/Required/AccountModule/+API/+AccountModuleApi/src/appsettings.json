{
  "ConnectionStrings": {
    "Active": "Data Source=./../../../__Data/Database/AccountModuleData.db"              
  },
  "Endpoints" : {
    "IdentityEndpointUrl" : "https://id.wordsearchkingdom.com",
    "IdentityValidIssuer" : "https://id.wordsearchkingdom.com",
    
    "PubsubEndpointUrl" : "https://pub.wordsearchkingdom.com",

    "AccountAdminApiUrl" : "https://account-api.wordsearchkingdom.com",
    "AccountAdminApiVersion" : "v1",
    "AccountAdminApiName" : "account_api",
    
    "WskApiUrl" : "https://wsk-api.wordsearchkingdom.com",
    "WskApiVersion" : "v1",
    "WskApiName" : "wsk_api",
    
    "WskAdminApiUrl" : "https://wsk-a-api.wordsearchkingdom.com",
    "WskAdminApiVersion" : "v1",
    "WskAdminApiName" : "wsk_admin_api"
