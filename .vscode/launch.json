            "args": [],
            "cwd": "${workspaceFolder}/src/+Modules/Required/AccountModule/+API/+AccountModuleApi/src",
            "stopAtEntry": false,
            "serverReadyAction": {
                "action": "openExternally",
                "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
